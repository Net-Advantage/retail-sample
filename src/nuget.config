<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<packageSources>
		<clear />
		<add key="NabsGitHubPackages" value="https://nuget.pkg.github.com/Net-Advantage/index.json" protocolVersion="3" />
		<add key="nuget.org" value="https://api.nuget.org/v3/index.json" protocolVersion="3" />
	</packageSources>
	<solution>
		<add key="disableSourceControlIntegration" value="true" />
	</solution>
	<packageSourceMapping>
		<clear />
		<packageSource key="NabsGitHubPackages">
			<package pattern="*" />
		</packageSource>
		<packageSource key="nuget.org">
			<package pattern="*" />
		</packageSource>
	</packageSourceMapping>
